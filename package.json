{
  "name": "loopcontrol",
  "version": "1.0.1",
  "description": "Firefox/Chrome extension to loop specified sections of videos. ",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build:firefox": "cp ./manifests/manifest.v2.json ./manifest.json && web-ext build -a './dist/' -i 'tests' 'manifests'",
    "start:firefox": "cp ./manifests/manifest.v2.json ./manifest.json && web-ext run --source-dir ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aprestoes/loopcontrol.git"
  },
  "author": "aprestoes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aprestoes/loopcontrol/issues"
  },
  "homepage": "https://github.com/aprestoes/loopcontrol#readme",
  "devDependencies": {
    "crx": "^5.0.1",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "puppeteer": "^24.4.0",
    "web-ext": "^8.6.0"
  },
  "dependencies": {
  }
}
